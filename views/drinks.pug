extends layout 

block content 
  h1= title
  -
    var results = [
      {
        drinks_name: 'Thumps Up',
        company_name: 'Thumps Up',
        drinks_quantity: 1
      },
      {
        drinks_name: 'Mountain Dew',
        company_name: 'Mountain Dew',
        drinks_quantity: 1
      },
      {
        drinks_name: 'Sprit',
        company_name: 'Sprit',
        drinks_quantity: 1
      }
    ];
  div.container-fluid
    div.row
      each drinks in results 
        div(class="col-sm-2") #{drinks.drinks_name} #{drinks.company_name} #{drinks.drinks_quantity}   